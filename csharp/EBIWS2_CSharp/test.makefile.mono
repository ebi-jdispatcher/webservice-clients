	${MONO} DaliliteCliClient/bin/Release/DaliliteCliClient.exe --params

dalilite_param_detail:
	${MONO} DaliliteCliClient/bin/Release/DaliliteCliClient.exe --paramDetail chainid1

dalilite_file: test_data
	${MONO} DaliliteCliClient/bin/Release/DaliliteCliClient.exe --email ${EMAIL} --structure1 ../test_data/pdb11as.ent --structure2 ../test_data/pdb12as.ent

dalilite_pdbid:
	${MONO} DaliliteCliClient/bin/Release/DaliliteCliClient.exe --email ${EMAIL} --structure1 'PDB:1A06' --structure2 'PDB:1A07' --chainid1 A --chainid2 A --outfile dalilite-pdbidinput

dalilite_clean:
	rm -f dalilite-*

# DbClustal
dbclustal: dbclustal_params dbclustal_param_detail \
dbclustal_file

dbclustal_params:
