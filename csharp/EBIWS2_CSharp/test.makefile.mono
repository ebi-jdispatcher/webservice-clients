clustalw2_align: test_data
	${MONO} ClustalW2CliClient/bin/Release/ClustalW2CliClient.exe --email ${EMAIL} ../test_data/multi_prot.tfa

clustalw2_align_stdin_stdout: test_data
	cat ../test_data/multi_prot.tfa | ${MONO} ClustalW2CliClient/bin/Release/ClustalW2CliClient.exe --email ${EMAIL} --quiet --outformat aln-clustalw --outfile - - > clustalw2-blah.aln

